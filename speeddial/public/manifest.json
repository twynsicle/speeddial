{
  "name": "Speeddial",
  "version": "1.0",
  "description": "Configurable Speeddial",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "converted_from_user_script": false,
  "options_page": "options/options.html",
  "background": {
    "scripts": ["js/libs/jquery-2.0.3.min.js", "js/libs/SettingsManager.js", "background/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "save page",
    "default_icon": "images/icons/save.png"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    "/config.json",
    "css/styles.scss",

    "options/options.html"
  ]

}